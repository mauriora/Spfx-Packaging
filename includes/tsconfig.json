{
  "extends": "@microsoft/rush-stack-compiler-4.5/includes/tsconfig-web.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "typeRoots": [
      "../../node_modules/@types", "../../node_modules/@microsoft"
    ],
    "types": [
      "es6-promise",
      "webpack-env"
    ],
    "lib": [
      "es2019",
      "dom",
    ]
  },
  "exclude": [
    "node_modules",
    "lib"
  ]
}
